	<Project>
		<Option title="GRBL" />
		<Option pch_mode="2" />
		<Option compiler="avr-gcc" />
		<Build>
			<Target title="all">
				<Option output="bin/Release/GRBL.elf" prefix_auto="1" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output="obj/Release/" />
				<Option type="5" />
				<Option compiler="avr-gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-O2" />
				</Linker>
			</Target>
			<Environment>
				<Variable name="MCU" value="atmega2560" />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-mmcu=atmega2560" />
		</Compiler>
		<Linker>
